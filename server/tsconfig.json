{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "outDir": "./dist",
      "moduleResolution": "node",
      "strict": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "./**/*.ts",
      "./**/*.d.ts"
    ],
    "exclude": ["node_modules", "**/*.spec.ts"],
  }
  